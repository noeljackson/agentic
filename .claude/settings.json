{
  "permissions": {
    "allow": [
      "Bash(*:*)",
      "Read(*)",
      "Write(*)",
      "Edit(*)"
    ],
    "deny": [
      "Bash(rm -rf /:*)",
      "Bash(rm -rf ~:*)",
      "Bash(mkfs:*)",
      "Bash(dd if=:*)",
      "Bash(chmod -R 777 /:*)",
      "Bash(git push --force origin main:*)",
      "Bash(git push --force origin master:*)",
      "Bash(shutdown:*)",
      "Bash(reboot:*)"
    ]
  }
}
